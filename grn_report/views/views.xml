<odoo>
    <data>


  <record id="list_grn_report_view" model="ir.ui.view">
             <field name="name">grn.report.view.list</field>
             <field name="model">grn.report.view</field>
             <field name="arch" type="xml">
                 <tree create="false">
                      <field name="origin"/>
                        <field name="name"/>
                        <field name="partner"/>
                        <field name="effective_date"/>
                        <field name="productname"/>
                        <field name="demanded_qty"/>
                        <field name="cost"/>
                        <field name="received_qty"/>
                        <field name="unit_price"/>
                 </tree>
             </field>
         </record>

         <record id="action_grn_report_view" model="ir.actions.act_window">
             <field name="name">GRN Report</field>
             <field name="res_model">grn.report.view</field>
             <field name="view_mode">tree</field>
              <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create
                </p>
            </field>
         </record>



