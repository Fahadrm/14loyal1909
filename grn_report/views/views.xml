        <record id="view_grn_report_excel" model="ir.ui.view">
            <field name="name">view.grn.report.excel</field>
            <field name="model">grn.report.excel</field>
            <field name="arch" type="xml">
                <form string="Generated xls File">
                    <group>
                        <field name="file_name" invisible="1"/>
                        <field name="excel_file" readonly="1" filename="file_name"/>
                    </group>
                    <footer>
                        <!--					 <button name="action_lost_reason_applys" string="Submit" type="object" class="btn-primary" data-hotkey="q"/>-->
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--	  wizard view-->
        <record id="view_grn_report_wizard" model="ir.ui.view">
            <field name="name">grn.report.wizard.view</field>
            <field name="model">grn.report.wizard</field>
            <field name="arch" type="xml">
                <form string="GRN Report">

                    <group colspan="4">
                        <group>
                            <field name="start_date"/>
                        </group>
                        <group>
                            <field name="end_date"/>
                        </group>

                    </group>
                    <group>
                        <field name="company_id"/>
                        <field name="vendor_id"/>
                    </group>


                    <footer>
                        <button id="button_submit" name="action_submit" string="Submit" class="btn-primary"
                                type="object"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

